{
    "name": "timeweb/phpstan-enum",
    "description": "Enum class reflection extension for PHPStan",
    "type": "phpstan-extension",
    "keywords": ["enum", "phpstan"],
    "license": "MIT",
    "require": {
        "php": "~7.1",
        "myclabs/php-enum": "^1.2",
        "phpstan/phpstan": "^0.10|^0.11|^0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Timeweb\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/_fixture/"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
